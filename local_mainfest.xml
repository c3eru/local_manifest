<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android">

	<remote
		name="pixel"
		fetch="https://github.com/PixelExperience"
		revision="thirteen"
		review="gerrit.pixelexperience.org"/>

	<remote
		name="mobx"
		fetch="https://github.com/MOBX-PROJECTS"/>

	<remote
		name="c3eru"
		fetch="https://github.com/c3eru"/>


	<!-- Trees for bengal -->
	<project
		path="device/xiaomi/citrus"
		name="device_xiaomi_citrus"
		revision="lineage-20"
		remote="mobx"/>

	<project
		path="device/xiaomi/sm6115-common"
		name="device_xiaomi_sm6115-common"
		revision="lineage-20"
		remote="mobx"/>

	<project
		path="device/xiaomi/sm6115-common-kernel"
		name="device_xiaomi_sm6115-common-kernel"
		revision="lineage-20"
		remote="mobx"/>

	<project
		path="vendor/xiaomi/citrus"
		name="vendor_xiaomi_citrus"
		revision="lineage-20"
		remote="mobx"/>

	<project
		path="vendor/xiaomi/sm6115-common"
		name="vendor_xiaomi_sm6115-common"
		revision="lineage-20"
		remote="mobx"/>

        <!-- Hardware for bengal -->
	<project path="hardware/qcom-caf/bengal/audio" name="hardware_qcom-caf_bengal_audio" remote="pixel" />
	<project path="hardware/qcom-caf/bengal/display" name="hardware_qcom-caf_bengal_display" remote="pixel" />
	<project path="hardware/qcom-caf/bengal/gps" name="hardware_qcom-caf_bengal_gps" remote="pixel" />
	<project path="hardware/qcom-caf/bengal/media" name="hardware_qcom-caf_bengal_media" remote="pixel" />

	<!-- Replacement -->
	<remove-project name="android_hardware_lineage_compat"/>
	<remove-project name="android_vendor_banana"/>

        <project path="vendor/banana" name="android_vendor_banana" remote="c3eru" />

</manifest>


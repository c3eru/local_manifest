<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android">

	<remote
		name="PainKiller3"
		fetch="https://github.com/PainKiller3"/>

	<remote
		name="etc"
		fetch="https://github.com/c3eru"/>

	<remote
		name="kdrag0n"
		fetch="https://github.com/kdrag0n"/>

	<remote
		name="xd"
		fetch="https://github.com/xdroid-oss"
		revision="thirteen"/>

	<project
		path="device/xiaomi/beryllium"
		name="device_xiaomi_beryllium-1"
		remote="etc"
		revision="thirteen"
		clone-depth="1"/>

	<project
		path="vendor/xiaomi"
		name="vendor_xiaomi"
		remote="PainKiller3"
		revision="thirteen"
		clone-depth="1"/>

	<project
		path="kernel/xiaomi/sdm845"
		name="kernel_xiaomi_sdm845"
		remote="PainKiller3"
		revision="thirteen"
		clone-depth="1"/>

	<project
		path="prebuilts/clang/host/linux-x86/clang-proton"
		name="proton-clang"
		remote="kdrag0n"
		revision="master"
		clone-depth="1"/>

	<project
		path="device/xiaomi/sdm845-common"
		name="device_xiaomi_sdm845-common"
		remote="etc"
		revision="thirteen"
		clone-depth="1"/>

	<remove-project
		name="android_hardware_lineage_compat"/>

	<project
		path="hardware/xiaomi"
		name="xd_hardware_xiaomi"
		remote="xd"/>

</manifest>

